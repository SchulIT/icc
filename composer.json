        "defuse/php-encryption": "^2.0",
        "doctrine/doctrine-bundle": "^2.3",
        "doctrine/doctrine-migrations-bundle": "^3.0.0",
        "doctrine/orm": "^3.0",
        "doctrineencryptbundle/doctrine-encrypt-bundle": "^6.0",
        "dragonmantank/cron-expression": "^3.4",
        "easycorp/easyadmin-bundle": "^4.0",
        "eightpoints/guzzle-bundle": "^8.2",
        "fakerphp/faker": "^1.8",
        "javiereguiluz/easyslugger": "^1.0",
        "jms/serializer-bundle": "^5.0",
        "jsvrcek/ics": "^0.8",
        "knplabs/knp-menu-bundle": "^3.0",
        "knplabs/knp-time-bundle": "^2.4",
        "league/commonmark": "^2.0",
        "league/csv": "^9.7",
        "league/flysystem": "^3.0",
        "lightsaml2/lightsaml": "^3.0",
        "lightsaml2/sp-bundle": "^5.0",
        "lightsaml2/symfony-bridge": "^6.0",
        "lorisleiva/cron-translator": "^0.4.6",
        "nelmio/api-doc-bundle": "^4.2",
        "oneup/flysystem-bundle": "^4.0",
        "ramsey/uuid-doctrine": "^2.0",
        "schulit/common-bundle": "^6.0",
        "scienta/doctrine-json-functions": "^6.0",
        "sensiolabs/ansi-to-html": "^1.2",
        "serhiy/pushover": "^1.3",
        "stof/doctrine-extensions-bundle": "^1.7",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "^7.0",
        "symfony/console": "^7.0",
        "symfony/doctrine-bridge": "^7.0",
        "symfony/doctrine-messenger": "^7.0",
        "symfony/dotenv": "^7.0",
        "symfony/error-handler": "^7.0",
        "symfony/expression-language": "^7.0",
        "symfony/flex": "^2.0",
        "symfony/form": "^7.0",
        "symfony/framework-bundle": "^7.0",
        "symfony/mailer": "^7.0",
        "symfony/messenger": "^7.0",
        "symfony/monolog-bundle": "^3.1",
        "symfony/process": "^7.0",
        "symfony/property-access": "^7.0",
        "symfony/property-info": "^7.0",
        "symfony/rate-limiter": "^7.0",
        "symfony/runtime": "^7.0",
        "symfony/scheduler": "^7.0",
        "symfony/security-bundle": "^7.0",
        "symfony/serializer": "^7.0",
        "symfony/string": "^7.0",
        "symfony/translation": "^7.0",
        "symfony/twig-bundle": "^7.0",
        "symfony/validator": "^7.0",
        "symfony/web-link": "^7.0",
        "symfony/webpack-encore-bundle": "^2.0",
        "symfony/yaml": "^7.0",
        "twig/cssinliner-extra": "^3.6",
        "twig/extra-bundle": "^3.6",
        "twig/inky-extra": "^3.6",
        "vich/uploader-bundle": "^2.0",
        "willdurand/js-translation-bundle": "^7.0",
        "xantios/mimey": "^2.1",
        "zenstruck/messenger-monitor-bundle": "^0.5.1"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^8.0",
        "league/flysystem-memory": "^3.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpdoc-parser": "^2.0",
        "phpstan/phpstan": "^1.7",
        "phpstan/phpstan-doctrine": "^1.3",
        "phpstan/phpstan-symfony": "^1.2",
        "phpunit/phpunit": "^9.0",
        "psr/event-dispatcher": "^1.0",
        "rector/rector": "^0.15.0",
        "symfony/browser-kit": "^7.0",
        "symfony/css-selector": "^7.0",
        "symfony/debug-bundle": "^7.0",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^7.0",
        "symfony/stopwatch": "^7.0",
        "symfony/web-profiler-bundle": "^7.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "phpstan/extension-installer": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^7.0"
        }
    }
}
